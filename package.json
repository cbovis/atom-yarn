{
  "name": "atom-yarn",
  "main": "./lib/atom-yarn",
  "version": "0.8.0",
  "description": "A handy little package that lets you do Yarn things without leaving Atom.",
  "author": "Craig Bovis <craig@bovis.me.uk>",
  "keywords": [
    "yarn"
  ],
  "repository": "https://github.com/cbovis/atom-yarn",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "lint": "eslint --quiet .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js, jsx}": [
